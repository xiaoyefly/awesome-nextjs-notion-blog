{
  "name": "nextjs-notion-starter-kit",
  "version": "0.1.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel --prod",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-s test:*",
    "test:lint": "eslint .",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "posttest": "run-s build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@google-cloud/firestore": "^6.5.0",
    "chrome-aws-lambda": "^5.5.0",
    "classnames": "^2.2.6",
    "colorthief": "^2.3.2",
    "comma-number": "^2.0.1",
    "date-fns": "^2.19.0",
    "device-uuid": "^1.0.4",
    "fathom-client": "^3.0.0",
    "got": "^11.8.1",
    "isomorphic-unfetch": "^3.1.0",
    "lqip-modern": "^1.1.3",
    "next": "^10.1.3",
    "node-fetch": "^2.6.1",
    "notion-client": "^4.11.1",
    "notion-types": "^4.10.0",
    "notion-utils": "^4.10.0",
    "p-map": "^4.0.0",
    "p-memoize": "^4.0.0",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-body-classname": "^1.3.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.1.0",
    "react-notion-x": "^4.11.3",
    "react-simple-snackbar": "^1.1.11",
    "react-static-tweets": "^0.5.3",
    "react-use": "^15.3.3",
    "smoothscroll-polyfill": "^0.4.4",
    "static-tweets": "^0.5.3",
    "swr": "^0.5.5",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.1.3",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.9",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "next-sitemap": "^1.6.203",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.0.5",
    "typescript": "^4.1.3"
  }
}
